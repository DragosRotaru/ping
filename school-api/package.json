{
  "name": "experiments-school-map-api",
  "version": "1.0.0",
  "description": "Finding and listing the nearby schools and their ratings in the area.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/askhuckleberry/experiments-school-map.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/askhuckleberry/experiments-school-map/issues"
  },
  "homepage": "https://github.com/askhuckleberry/experiments-school-map#readme",
  "scripts": {
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "ts-node src/index.ts",
    "debug": "node --inspect -r ts-node/register src/index.ts",
    "dev": "npx nodemon"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "tslint": {
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-jest",
      "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {},
    "rulesDirectory": []
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.11",
    "@types/connect-ensure-login": "^0.1.4",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.5",
    "@types/express-session": "^1.15.12",
    "@types/helmet": "0.0.43",
    "@types/imap": "^0.8.29",
    "@types/jest": "^24.0.11",
    "@types/joi": "^14.3.3",
    "@types/mongodb": "^3.1.26",
    "@types/node": "^11.13.8",
    "@types/prettyjson": "0.0.29",
    "@types/request": "^2.48.1",
    "@types/uuid": "^3.4.4",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-jest": "^1.0.5",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.16.1",
    "fluent-logger": "^3.3.1",
    "helmet": "^3.16.0",
    "imap": "^0.8.19",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "joi": "^14.3.1",
    "moment": "^2.24.0",
    "prettyjson": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "serve-static": "^1.13.2",
    "uuid": "^3.3.2"
  }
}
