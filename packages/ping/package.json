{
  "name": "@huckleberryai/ping",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "restricted"
  },
  "version": "3.2.3",
  "description": "Ping Domain",
  "author": "Dragos Rotaru <dragos@ping.buzsz>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huckleberrylabs/ping.git"
  },
  "bugs": {
    "url": "https://github.com/huckleberrylabs/ping/issues"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "src"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "tslint": {
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-jest",
      "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {},
    "rulesDirectory": []
  },
  "scripts": {
    "build": "npx tsc",
    "test": "npx jest",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test"
  },
  "dependencies": {
    "@huckleberryai/core": "^3.2.3",
    "fp-ts": "^2.1.0",
    "io-ts": "^2.0.1"
  },
  "devDependencies": {
    "@types/color": "^3.0.0",
    "@types/humanparser": "^1.1.1",
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.6",
    "@types/uuid": "^3.4.6",
    "jest": "^24.8.0",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-jest": "^1.0.5",
    "typescript": "^3.7.2"
  },
  "gitHead": "f3a4f0b4f5d24587cfc9f288936eb8ab206ae0a9"
}
