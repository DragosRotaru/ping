{
  "name": "@huckleberrylabs/ping-client",
  "private": true,
  "version": "3.2.8",
  "description": "Ping Client",
  "author": "Dragos Rotaru <dragos@ping.buzz>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huckleberrylabs/ping.git"
  },
  "bugs": {
    "url": "https://github.com/huckleberrylabs/ping/issues"
  },
  "main": "dist/ping.min.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "sideEffects": false,
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "tslint": {
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-jest",
      "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {},
    "rulesDirectory": []
  },
  "scripts": {
    "start": "NODE_ENV=development npx webpack --config webpack.config.js",
    "build": "npx webpack --config webpack.config.js",
    "test": "npx jest",
    "preinstall": "node ./scripts/netlify-preinstall.js"
  },
  "dependencies": {
    "@huckleberrylabs/ping-core": "^3.2.7",
    "axios": "^0.19.0",
    "dotenv": "^8.2.0",
    "fp-ts": "^2.6.7",
    "io-ts": "^2.2.7"
  },
  "devDependencies": {
    "@types/color": "^3.0.0",
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.6",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-jest": "^1.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.10",
    "webpack-obfuscator": "^0.18.3"
  },
  "gitHead": "c82709737c51f8029b23824a79de2f34723a8e4b"
}
