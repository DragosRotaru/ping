{
  "version": 2,
  "name": "@huckleberryai/widget-client",
  "scope": "huckleberryai",
  "alias": ["static.huckleberry.app"],
  "public": false,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "build": {
    "env": {
      "NPM_TOKEN": "@npm-token",
      "WIDGET_ID": "@widget-id",
      "NODE_ENV": "@node-env"
    }
  }
}
