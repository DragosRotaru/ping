{
  "name": "@huckleberryai/widget-client",
  "publishConfig": {
    "access": "restricted"
  },
  "version": "0.2.8",
  "description": "Text, By Huckleberry",
  "author": "Dragos Rotaru <dragos@rotaru.co>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/askhuckleberry/monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/askhuckleberry/monorepo/issues"
  },
  "main": "dist/text.min.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "tslint": {
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-jest",
      "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {},
    "rulesDirectory": []
  },
  "scripts": {
    "deploy:prod": "now --target production",
    "deploy:staging": "now -e NODE_ENV=staging --target staging",
    "deploy:dev": "now dev",
    "now-build": "npm run build",
    "build": "npx webpack --config webpack.config.js",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "prepare": "npm run test && npm run build"
  },
  "dependencies": {
    "@huckleberryai/core": "^0.2.7",
    "@huckleberryai/log": "^0.2.7",
    "@huckleberryai/web-analytics": "^0.2.8",
    "@huckleberryai/widget": "^0.2.7",
    "axios": "^0.19.0",
    "fp-ts": "^2.1.0",
    "io-ts": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/node": "^12.6.8",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "now": "^16.1.1",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-jest": "^1.0.5",
    "typescript": "^3.5.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-obfuscator": "^0.18.3"
  },
  "gitHead": "62a71a063a7992eb35f5a1be639ebe138876d8f6"
}
