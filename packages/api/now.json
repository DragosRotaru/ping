{
  "version": 2,
  "name": "@huckleberry/api",
  "alias": ["api.huckleberry.app"],
  "scope": "huckleberryai",
  "public": false,
  "builds": [{ "src": "src/index.ts", "use": "@now/node" }],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/index.ts?zeit_now_route=$1"
    }
  ],
  "build": {
    "env": {
      "REACT_APP_PUBLIC_GOOGLE_API_KEY": "@public-google-api-key",
      "TEXT_TEST_APP_ID": "@text-test-app-id"
    }
  },
  "env": {
    "TWILIO_ACCOUNT_SID": "@twilio-account-sid",
    "TWILIO_AUTH_TOKEN": "@twilio-auth-token",
    "TWILIO_PHONE_NUMBER": "@twilio-phone-number",
    "GOOGLE_API_KEY": "@google-api-key",
    "LOCAL_LOGIC_API_KEY": "@local-logic-api-key"
  }
}
