{
  "name": "@huckleberrylabs/core",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "restricted"
  },
  "version": "3.2.3",
  "description": "Ping Core",
  "author": "Dragos Rotaru <dragos@ping.buzz>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huckleberrylabs/ping.git"
  },
  "bugs": {
    "url": "https://github.com/huckleberrylabs/ping/issues"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "src"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "tslint": {
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-jest",
      "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {},
    "rulesDirectory": []
  },
  "scripts": {
    "build": "npx tsc",
    "test": "npx jest",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "color": "^3.1.2",
    "fp-ts": "^2.1.0",
    "http-status-codes": "^1.4.0",
    "humanparser": "^1.11.0",
    "io-ts": "^2.0.1",
    "libphonenumber-js": "^1.7.23",
    "moment": "^2.24.0",
    "normalize-url": "^4.5.0",
    "parse-full-name": "^1.2.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/color": "^3.0.0",
    "@types/humanparser": "^1.1.1",
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.6",
    "@types/normalize-url": "^4.2.0",
    "@types/uuid": "^3.4.6",
    "jest": "^24.8.0",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-jest": "^1.0.5",
    "typescript": "^3.7.2"
  },
  "gitHead": "c93fa7310f05e605cbccf873b8517b7d1189fc2f"
}
