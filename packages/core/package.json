{
  "name": "@huckleberryai/core",
  "publishConfig": {
    "access": "restricted"
  },
  "version": "0.1.20",
  "description": "Core Abstractions",
  "author": "Dragos Rotaru <dragos@rotaru.co>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/askhuckleberry/monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/askhuckleberry/monorepo/issues"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "src"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "tslint": {
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-jest",
      "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {},
    "rulesDirectory": []
  },
  "scripts": {
    "build": "npx tsc",
    "build:watch": "npx tsc --watch",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "prepare": "npm run test && npm run build"
  },
  "dependencies": {
    "http-status-codes": "^1.3.2",
    "moment": "^2.24.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.6.8",
    "@types/uuid": "^3.4.5",
    "jest": "^24.8.0",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-jest": "^1.0.5",
    "typescript": "^3.5.3"
  },
  "gitHead": "fc47f385d44fdf82c98cb27f2dc7ce2c50128342"
}
