{
  "name": "@huckleberryai/web-analytics",
  "publishConfig": {
    "access": "restricted"
  },
  "version": "0.2.8",
  "description": "Web Analytics",
  "author": "Dragos Rotaru <dragos@rotaru.co>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/askhuckleberry/monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/askhuckleberry/monorepo/issues"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "src"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "tslint": {
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-jest",
      "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {},
    "rulesDirectory": []
  },
  "dependencies": {
    "@huckleberryai/core": "^0.2.7",
    "@huckleberryai/log": "^0.2.7",
    "@now/node": "^1.0.0",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/color": "^3.0.0",
    "@types/humanparser": "^1.1.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.6.8",
    "@types/uuid": "^3.4.5",
    "jest": "^24.8.0",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-jest": "^1.0.5",
    "typescript": "^3.5.3"
  },
  "gitHead": "62a71a063a7992eb35f5a1be639ebe138876d8f6"
}
